<p id="notice"><%= notice %></p>

<h1>Listing Alunos</h1>

<table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Email</th>
      <th>Senha</th>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Fone1</th>
      <th>Fone2</th>
      <th>Logradouro</th>
      <th>Cidade</th>
      <th>Uf</th>
      <th>Cep</th>
      <th>Escola</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.login %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.senha %></td>
        <td><%= aluno.nome %></td>
        <td><%= aluno.cpf %></td>
        <td><%= aluno.fone1 %></td>
        <td><%= aluno.fone2 %></td>
        <td><%= aluno.logradouro %></td>
        <td><%= aluno.cidade %></td>
        <td><%= aluno.uf %></td>
        <td><%= aluno.cep %></td>
        <td><%= aluno.escola %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno', new_aluno_path %>
